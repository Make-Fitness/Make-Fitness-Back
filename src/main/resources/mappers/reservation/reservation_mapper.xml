<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="korit.com.make_fitness.mapper.ReservationMapper">

    <insert id="insertReservation" keyProperty="reservationId" useGeneratedKeys="true">
        insert into reservation_tb
        values(default, #{customerId}, #{classId}, #{bookingDateTime}, #{bookingStatus}, now(), now())
    </insert>
</mapper>